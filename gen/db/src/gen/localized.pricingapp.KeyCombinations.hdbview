VIEW localized_pricingapp_KeyCombinations AS SELECT
  L_0.name,
  coalesce(localized_1.descr, L_0.descr) AS descr,
  L_0.ID
FROM (pricingapp_KeyCombinations AS L_0 LEFT JOIN pricingapp_KeyCombinations_texts AS localized_1 ON localized_1.ID = L_0.ID AND localized_1.locale = SESSION_CONTEXT('LOCALE'))